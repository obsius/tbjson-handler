{
	"name": "tbjson-handler",
	"version": "1.0.8",
	"description": "A TBJSON handler for event propagation.",
	"homepage": "https://github.com/obsius/tbjson-handler",
	"author": "Jeff Seaman <mr.jeffrey.seaman@gmail.com> (https://www.ngc6822.com)",
	"license": "MIT",
	"main": "lib/index.js",
	"scripts": {
		"prepare": "rollup -c",
		"start": "nodemon --inspect dev.js --exec babel-node"
	},
	"dependencies": {
		"typed-binary-json": "^1.17.4"
	},
	"devDependencies": {
		"@babel/core": "^7.12.10",
		"@babel/node": "^7.12.10",
		"@babel/plugin-proposal-class-properties": "^7.12.1",
		"@babel/preset-env": "^7.12.11",
		"nodemon": "^2.0.6",
		"rollup": "^2.35.1",
		"rollup-plugin-babel": "^4.4.0"
	},
	"babel": {
		"plugins": [
			"@babel/plugin-proposal-class-properties"
		],
		"presets": [
			[
				"@babel/preset-env",
				{
					"targets": {
						"node": "current"
					}
				}
			]
		]
	},
	"keywords": [
		"tbjson",
		"handler",
		"event",
		"listener",
		"tree",
		"binary",
		"typed"
	]
}
